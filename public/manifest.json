{
  "manifest_version": 3,
  "name": "DeepBlock",
  "description": "🧘🏻‍♂️ BlockSiteZen : block and regain your freedom !",
  "version": "1.0.0",
  "icons": {
    "128": "icon-128x128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://www.google.com/*"
  ]
}